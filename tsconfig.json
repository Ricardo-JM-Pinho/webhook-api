{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["esnext.asynciterable", "es2017", "dom"],
    "module": "commonjs",
    "sourceMap": true,
    "strict": false,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "removeComments": true,
    "allowJs": true,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist"
  },
  "exclude": ["node_modules", "dist"],
  "include": ["server"]
//  "references": [
